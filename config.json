{
  "name": "Home Assistant OpenAPI Server",
  "version": "3.0.0",
  "slug": "ha-openapi-server",
  "description": "77 production-ready FastAPI endpoints for HA control with Open-WebUI",
  "arch": ["armhf", "armv7", "aarch64", "amd64", "i386"],
  "startup": "application",
  "boot": "auto",
  "init": false,
  "map": ["config:rw"],
  "options": {
    "port": 8001,
    "log_level": "info",
    "admin_token": ""
  },
  "schema": {
    "port": "port",
    "log_level": "list(debug|info|warning|error|critical)",
    "admin_token": "str?"
  },
  "ports": {
    "8001/tcp": 8001
  },
  "ports_description": {
    "8001/tcp": "OpenAPI Server HTTP endpoint"
  },
  "homeassistant_api": true,
  "hassio_api": true,
  "hassio_role": "default",
  "auth_api": false,
  "ingress": false,
  "panel_icon": "mdi:server-network",
  "panel_title": "OpenAPI Server"
}
